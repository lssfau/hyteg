Parameters
{
  // those are the fine grid levels for the convergence tests, not the v-cycle hierarchy levels
  minLevel 2;
  maxLevel 3;

  // "cube" or "torus"
  domain torus;

  // "vcycles" or "fmg"
  solverType fmg;

  preSmooth     1;
  postSmooth    1;
  numVCyclesFMG 5;

  coarseGridRefinements 0;

  toroidalResolution 34;
  poloidalResolution 6;
  tubeLayerRadius 0.4;

  vtk false;
  timingJSON true;

  baseName curlcurl;
}
